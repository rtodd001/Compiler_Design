function main;
beginparams
endparams
beginlocals
	a : array[10] of integer;
	i , n : integer;
endlocals
beginbody
	read n;
    i := 0;
	do
    beginloop
        a[i] := i * 2;
        write a[i];
        i := i + 1;
        continue;
        write i;
    endloop
    while i < n;
endbody