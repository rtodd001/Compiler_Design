function main;
beginparams
endparams
beginlocals
	a : array[20] of integer;
	i , n : integer;
endlocals
beginbody
	read n;
    i := 0;
	while i <= n or false
    beginloop
        a[i] := i * 2;
        write a[i];
        i := i + 1;
        continue;
        write i;
    endloop;
endbody